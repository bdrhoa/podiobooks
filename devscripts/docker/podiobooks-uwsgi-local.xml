<uwsgi>
	<workers>2</workers>
    <env>DEBUG=True</env>
    <!-- Note that SECRET_KEY can't have any ^ or @ or ( )
    <env>SECRET_KEY=></env>
    <env>EMAIL_BACKEND=django.core.mail.backends.smtp.EmailBackend</env>
    <env>EMAIL_HOST=</env>
    <env>EMAIL_HOST_USER=</env>
    <env>EMAIL_HOST_PASSWORD=</env>
    -->
    <env><![CDATA[DEFAULT_FROM_EMAIL="Podiobooks DEV <podiobooksdev@gmail.com>"]]></env>
    <env><![CDATA[SERVER_EMAIL="Podiobooks DEV <podiobooksdev@gmail.com>"]]></env>
    <env>MANAGERS="(('Podiobooks DEV', 'podiobooksdev@gmail.com'),)"</env>
    <env>ADMINS="(('Podiobooks DEV', 'podiobooksdev@gmail.com'), ('Brant', 'brant.steen@gmail.com'))"</env>
    <env>SEND_BROKEN_LINK_EMAILS=False</env>
    <env>GOOGLE_ANALYTICS_ID=None</env>
    <env>CACHE_MIDDLEWARE_SECONDS=5</env>
    <env>INSTANCE_TYPE=development</env>
    <env>MEDIA_DOMAIN=dev.media.podiobooks.com</env>
    <env>FEED_DOMAIN=dev.feeds.podiobooks.com</env>
    <!--
    <env>FEED_CACHE_TOKEN=</env>
    <env>FEED_CACHE_SECRET=</env>
    <env>FEED_CACHE_ENDPOINT=http://endpoints.podiobooks.com/caching/url/</env>
    -->
</uwsgi>
