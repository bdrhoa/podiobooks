<uwsgi>
	<socket>/opt/podiobooks/data/uwsgi.sock</socket>
	<chmod-socket>666</chmod-socket>
	<chdir>/opt/podiobooks/data/podiobooks</chdir>
	<virtualenv>/opt/podiobooks/data/podiobooks/.env</virtualenv>
	<master>true</master>
	<pidfile>/opt/podiobooks/data/uwsgi.pid</pidfile>
	<enable-threads>true</enable-threads>
	<logto>/opt/podiobooks/data/uwsgi.log</logto>
	<wsgi-file>/opt/podiobooks/data/podiobooks/podiobooks/wsgi_docker.py</wsgi-file>
    <if-file>/opt/podiobooks/data/podiobooks-uwsgi-local.xml</if-file>
        <xml>/opt/podiobooks/data/podiobooks-uwsgi-local.xml</xml>
    <endif/>
</uwsgi>
